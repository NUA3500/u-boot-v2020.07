/dts-v1/;
#include "nua3500.dtsi"

/ {
	model = "Nuvoton NUA3500 EVB board";
	compatible = "nuvoton,nua3500";

	chosen {
		stdout-path = "serial0:9600n8";
	};

	memory {
		device_type = "memory";
		reg = <0xd0000000 0x2000000>;
	};

	aliases {
		serial0 = &uart0;
	};

};

